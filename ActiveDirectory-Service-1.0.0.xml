            <description></description>
            <script>rem NAME
rem  configure.bat
rem
rem LAST MODIFIED
rem  4-October-2012
rem
rem SYNOPSIS
rem
rem DESCRIPTION
rem   This script configures Active Directory Set up
rem
rem USAGE
rem
rem LICENSE
rem
rem AUTHOR
rem Shweta Malhotra
rem Persistent Systems Limited

@echo off

rem Checking for the existence for dcpromo
IF EXIST C:\Windows\System32\dcpromo.exe (

rem Validate Mandatory Arguments for AD Setup
rem mandatory_args  dnsname netbios password
if "x%dnsname%"=="x"  ( 
set errormessage="Mandatory Parameter dnsname is not defined for ADSetup"
echo %errormessage%
exit /B 1
)
if "x%safepass%"=="x" ( 
set errormessage="Mandatory Parameter SafeModeAdminPassword is not defined for ADSetup"
echo %errormessage%
exit /B 1
)
echo "Configuring Active Directory with properties:"
echo ReplicaorNewDomain:Domain
echo NewDomain:Forest
echo ForestLevel:4
echo DomainLevel:4
echo InstallDNS:Yes
echo NewDomainDNSName: %dnsname%
echo DomainNetbiosName:%netbios%
echo DatabasePath:%dbpath%
echo LogPath:%logpath%
echo SYSVOLPath:%svpath%
echo SafeModeAdminPassword:%safepass%

rem AD-Setup with dcpromo
echo "Installing AD"
C:\Windows\System32\dcpromo.exe /unattend /ReplicaorNewDomain:Domain /NewDomain:Forest /NewDomainDNSName=%dnsname% /ForestLevel:4 /DomainLevel:4 /InstallDNS:Yes /DomainNetbiosName:%netbios% /DatabasePath:%dbpath% /LogPath:%logpath% /SYSVOLPath:%svpath% /SafeModeAdminPassword:%safepass% /RebootOnCompletion:No /RebootOnSuccess:No
) ELSE (
echo "File does not exist"
)
IF %ERRORLEVEL% LEQ 10 (
echo "AD Installed Successfully"
exit /B 0
 ) ELSE (
 echo "AD is not installed successfully"
 exit /B 1
 ) </script>
