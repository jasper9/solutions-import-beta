            <script>rem NAME
rem   install.bat
rem
rem LAST MODIFIED
rem  4-October-2012   
rem
rem SYNOPSIS
rem  This script test the supported OS and changes the Admin Passsword
rem
rem LICENSE
rem AUTHOR
rem  Shweta Malhotra
rem Persistent Systems Limited

echo Test for the supported OS
@echo off
systeminfo | find "OS Name" &gt; %TEMP%\osname.txt
FOR /F "usebackq delims=: tokens=2" %%i IN (%TEMP%\osname.txt) DO set vers=%%i
echo %vers% | find "Windows Server 2008" &gt; nul
if %ERRORLEVEL% == 0 (
echo "Supported OS"
) ELSE (
echo UnSupported OS"
exit /B 1
)
echo Changing local admin password
net user Administrator %password%
</script>
