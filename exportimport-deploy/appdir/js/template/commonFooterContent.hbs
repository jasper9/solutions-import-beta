<script src="http://code.jquery.com/jquery-1.8.2.min.js"></script>
<!--<script src="http://cdn.jsdelivr.net/fancybox/2.1.3/jquery.fancybox.pack.js"></script>-->
<script>
    $("#nsWrapper").find("img").each(function (i, img) {
        $(img).addClass("thumbnail").click(function (e) {
            e.preventDefault();
            window.open(this.src, "", "status=no,toolbar=no,menubar=no,location=no,resizable=no")
        });
    });
</script>